<?xml version="1.0" encoding="UTF-8"?>
<urlrewriting>
	<rules>
		<rule documentModel="faq" viewMode="detail">
			<template>/faq/$label-$lang,$id</template>
		</rule>

		<rule redirection="faq/ViewList">
			<template>/faq/list/</template>
		</rule>
		
		<rule redirection="faq/ViewList" lang="fr">
			<template>/faq/liste/</template>
		</rule>
	</rules>
</urlrewriting>